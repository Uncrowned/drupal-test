# user specific properties


env.db.driver=mysql
env.db.host=%DB HOST%
env.db.name=%DB NAME%
env.db.user=%DB USER%
env.db.password=%DB PASSWORD%
env.db.user.sudo=%DB USER%
env.db.user.sudo.password=%DB PASSWORD%

env.server=apache2

env.host.file=/etc/hosts
env.host.name=%HOST NAME%
env.host.ip=127.0.1.1
env.host.vhost_dir.avaliable=/etc/apache2/sites-available
env.host.vhost_dir.enabled=/etc/apache2/sites-enabled
env.host.creation.use_sudo=true

;env.proxy=192.168.1.2:3128
env.proxy=

env.deploy.package.profile=dev.server

env.java="/usr/bin/java"

env.environment=development
env.solr.start=0

env.solr.server=default

env.memcache = 0
env.memcache.tpl = default
env.memcache.prefix = default

env.cdn = 0
env.cdn.tpl = default

env.log_dir=../resources/logs
